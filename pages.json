{
	"pages": [{
		"path": "pages/index/index",
		"style": {
			"navigationBarTitleText": "index"
		}
	}, {
		"path": "pages/help/help",
		"style": {
			"navigationBarTitleText": "help"
		}
	}    ,{
            "path" : "pages/help/help",
            "style" : {}
        }
    ],
	"globalStyle": {
		"navigationBarBackgroundColor": "#CCCCCC",
		"navigationBarTextStyle": "white",
